# Multi-Agent Example - Environment Configuration

# Application
NODE_ENV=production

# Agent Configuration
AGENT_NAME=orchestrator
AGENT_ROLE=coordinator

# RabbitMQ
RABBITMQ_URL=amqp://reclapp:reclapp@rabbitmq:5672

# Redis
REDIS_URL=redis://redis:6379

# Event Store
EVENTSTORE_URL=esdb://eventstore:2113?tls=false

# Database
DATABASE_URL=postgresql://reclapp:reclapp@postgres:5432/reclapp

# Orchestrator
ORCHESTRATOR_URL=http://orchestrator:8080
CONFLICT_RESOLUTION=weighted_voting
AGENTS=risk-agent:8081,compliance-agent:8082,customer-agent:8083

# Compliance Agent
REGULATIONS=GDPR,AML,KYC

# Grafana (optional - monitoring profile)
GRAFANA_PASSWORD=admin
