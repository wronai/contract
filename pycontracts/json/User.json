{
  "description": "Represents a user record.",
  "properties": {
    "age": {
      "anyOf": [
        {
          "maximum": 150,
          "minimum": 0,
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Optional age",
      "examples": [
        34
      ],
      "title": "Age"
    },
    "created_at": {
      "description": "Creation timestamp (RFC3339)",
      "examples": [
        "2026-01-01T12:00:00Z"
      ],
      "format": "date-time",
      "title": "Created At",
      "type": "string"
    },
    "email": {
      "description": "User email address",
      "examples": [
        "alice@example.com"
      ],
      "format": "email",
      "title": "Email",
      "type": "string"
    },
    "id": {
      "description": "Unique identifier",
      "format": "uuid",
      "title": "Id",
      "type": "string"
    },
    "name": {
      "description": "Display name",
      "examples": [
        "Alice"
      ],
      "maxLength": 120,
      "minLength": 1,
      "title": "Name",
      "type": "string"
    }
  },
  "required": [
    "id",
    "email",
    "name",
    "created_at"
  ],
  "title": "User",
  "type": "object"
}
