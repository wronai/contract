{
  "$schema": "template",
  "description": "API package.json fallback template",
  "variables": ["name", "version"],
  "template": {
    "name": "{{name}}",
    "version": "{{version}}",
    "scripts": {
      "dev": "ts-node src/server.ts",
      "start": "node dist/server.js",
      "build": "tsc"
    },
    "dependencies": {
      "cors": "^2.8.5",
      "express": "^4.18.2"
    },
    "devDependencies": {
      "@types/cors": "^2.8.14",
      "@types/express": "^4.17.20",
      "@types/node": "^20.9.0",
      "ts-node": "^10.9.2",
      "typescript": "^5.3.0"
    }
  }
}
